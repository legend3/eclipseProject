<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- namespace:该mapper.xml映射文件的 唯一标识 -->
<mapper namespace="org.legend.springMybatis.mapper.StudentMapper">
	<insert id="addStudent" parameterType="org.legend.springMybatis.entity.Student">
		insert into student(stuno,stuname,stuage) values (#{stuNo},#{stuName},#{stuAge})
	</insert>
</mapper>
